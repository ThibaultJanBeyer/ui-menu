{
  "name": "tjb-menu",
  "version": "0.0.1",
  "description": "awesome menu webcomponent",
  "main": "tjb-menu.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "babel --watch tjb-menu.js --out-dir docs",
    "build": "npm run build-max | npm run build-min",
    "build-max": "babel tjb-menu.js --out-file docs/tjb-menu.js",
    "build-min": "babel --minified tjb-menu.js --out-file docs/tjb-menu.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThibaultJanBeyer/ui-menu.git"
  },
  "keywords": [
    "webcomponent",
    "custom",
    "element",
    "htmlelement",
    "html",
    "menu",
    "menubar"
  ],
  "author": "Thibault Jan Beyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThibaultJanBeyer/ui-menu/issues"
  },
  "homepage": "https://github.com/ThibaultJanBeyer/ui-menu#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0"
  }
}
